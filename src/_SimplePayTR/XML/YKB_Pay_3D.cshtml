    <input name="posnetID" type="hidden" id="PosnetID" value="@(Model.Account.PosnetId)">

    <input name="posnetData" type="hidden" id="posnetData" value="@(Model.Pay.FullName)">
    <input name="posnetData2" type="hidden" id="posnetData2" value="@(Model.Pay.EMail)">
    <input name="digest" type="hidden" id="sign" value="@(Model.Pay.Ip)">

    <input name="vftCode" type="hidden" id="vftCode" value="">
    <input name="useJokerVadaa" type="hidden" id="useJokerVadaa" value="0">
    <input name="merchantReturnURL" type="hidden" id="merchantReturnURL" value="@(Model.Extra.SuccessUrl)">
